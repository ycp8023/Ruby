<div id="<%= dom_id transaction_order %>">
<% if user_signed_in? %>
  <p>
    <strong>订单创建人：</strong>
    <%= current_user.id%>
  </p>
<% end %>
<% if admin_signed_in? %>
    <p>
      <strong>订单创建用户id：</strong>
      <%= transaction_order.user.id%>
    </p>
<% end %>
  <p>
    <strong>购买产品</strong>
    <%= transaction_order.cart_item.product.product_name%>
  </p>

  <p>
    <strong>收货地址</strong>
    <%= transaction_order.delivery_address %>
  </p>

  <p>
    <strong>收件人</strong>
    <%= transaction_order.delivery_name %>
  </p>

  <p>
    <strong>电话</strong>
    <%= transaction_order.delivery_phone %>
  </p>

  <p>
    <strong>订单备注</strong>
    <%= transaction_order.delivery_postcode %>
  </p>
</div>
